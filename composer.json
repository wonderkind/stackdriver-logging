{
	"name": "wonderkind/stackdriver-logging",
	"description": "Log to Google Stackdriver Logging using Monolog",
	"authors": [
		{
			"name": "Wonderkind Technologies B.V.",
			"email": "devops@wonderkind.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Wonderkind\\StackdriverLogging\\": "src/",
			"Wonderkind\\StackdriverLogging\\Test\\": "tests"
		}
	},
	"minimum-stability": "stable",
	"require": {
		"google/cloud-logging": "^1.24",
		"monolog/monolog": "^2.5.0",
		"psr/log": "2.0.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"orchestra/testbench": "^3.9.4"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Wonderkind\\StackdriverLogging\\StackdriverLoggerServiceProvider"
			]
		}
	}
}
